<resources>
    <declare-styleable name="numberThumbnail">
        
        <attr name="numberThumbnail" format="integer"/>
        <attr name="Thumbnail_width" format="integer"/>
        <attr name="Thumbnail_height" format="integer"/>
        <attr name="HighlightThumbnailImage" format="reference"/>
        <attr name="NormalThumbnailImage" format="reference"/>
         <attr name="isEditable" format="boolean"/>
        
        <!-- 0 = top, 1 = middle, 2 = bottom -->
        <attr name="layout_location" format="integer"/>
        
    </declare-styleable>
</resources>